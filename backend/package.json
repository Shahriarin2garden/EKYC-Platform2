{
  "name": "ekyc-backend",
  "version": "1.0.0",
  "description": "EKYC System - Backend API Server",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:pdf": "node src/services/testPdfGeneration.js",
    "test:pdf-quick": "node src/services/quickPdfTest.js",
    "test:pdf-rabbitmq": "node src/services/testRabbitMQPdf.js",
    "test:pdf-all": "node src/services/quickPdfTest.js && node src/services/testPdfGeneration.js"
  },
  "keywords": [
    "kyc",
    "api",
    "backend"
  ],
  "author": "Shahriar Hossain",
  "license": "MIT",
  "dependencies": {
    "@openrouter/sdk": "^0.1.10",
    "amqplib": "^0.10.9",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "mongoose": "^8.0.0",
    "pdfkit": "^0.17.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.3",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/services/test*.js",
      "!src/services/quick*.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
